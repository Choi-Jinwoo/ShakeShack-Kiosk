<Window x:Class="ShakeShack_Kiosk.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ShakeShack_Kiosk"
        xmlns:controls="clr-namespace:ShakeShack_Kiosk.Controls"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <controls:TitleControl x:Name="titleControl"/>
            <Frame x:Name="frPage"
                   Source="/View/LoginView.xaml"
                   NavigationUIVisibility="Hidden" />
        </StackPanel>
    </Grid>
</Window>
